datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider             = "prisma-client-py"
  interface            = "asyncio"
  recursive_type_depth = 5
}

model Books {
  id          Int    @id @default(autoincrement())
  title       String @unique
  author      String
  Description String
  issue_date  String
  price       Int
  category    String
}
